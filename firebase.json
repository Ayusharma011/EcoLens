{
  "firestore": {
    "database": "(default)",
    "location": "asia-east1",
    "rules": "firestore.rules",
    "indexes": "firestore.indexes.json"
  },
  "functions": [
    {
      "source": "functions",
      "codebase": "default",
      "disallowLegacyRuntimeConfig": true,
      "ignore": [
        "node_modules",
        ".git",
        "firebase-debug.log",
        "firebase-debug.*.log",
        "*.local"
      ]
    }
  ],
  "emulators": {
  "auth": {
    "port": 9099
  },
  "firestore": {
    "port": 8081
  },
  "functions": {
    "port": 5001
  },
  "ui": {
    "enabled": true,
    "port": 4000
  }
  }
}
